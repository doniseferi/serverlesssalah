{
  "name": "severlesssalah",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "pretty": "prettier --config .prettierrc.json --write \"src/**/*.{js,ts,json,md}\"",
    "build": "npm run clean && tsc && copyfiles --up 1 \"src/**/*.json\" dist && copyfiles package.json npm.lock dist",
    "clean": "npm install --frozen-lockfile && rimraf dist",
    "start": "npm run build && cd dist\\functions && func host start",
    "test": "jest --testRegex \"\\.test\\.ts$\"",
    "test-save-results": "cross-env JEST_JUNIT_OUTPUT=test-results/test-results-local.xml jest --ci --runInBand --reporters=default --reporters=jest-junit --testRegex \"\\.test\\.ts$\""
  },
  "dependencies": {
    "salahtimes": "^1.1.14"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.2",
    "@fluffy-spoon/substitute": "^1.145.0",
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
