{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start host and attach debugger",
      "type": "node",
      "request": "attach",
      "timeout": 20000,
      "port": 9229,
      "preLaunchTask": "startFunctionsHost"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "disableOptimisticBPs": true,
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": ["-i"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
